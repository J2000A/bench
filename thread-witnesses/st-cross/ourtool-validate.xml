<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.9//EN" "https://www.sosy-lab.org/benchexec/benchmark-2.3.dtd">
<benchmark tool="goblint" timelimit="60 s" hardtimelimit="64 s" memlimit="1 GB" cpuCores="1">

  <option name="--conf">conf/svcomp-yaml-validate.json</option>
  <option name="--enable">dbg.timing.enabled</option>
  <option name="--set">ana.activated[+]</option><option>apron</option>
  <option name="--set">ana.apron.domain</option><option>polyhedra</option>

  <requiredfiles>${benchmark_path_abs}/${rundefinition_name}/${taskdef_name}.witness.yml</requiredfiles>
  <option name="--witness.yaml.unassume">${benchmark_path_abs}/${rundefinition_name}/${taskdef_name}.witness.yml</option>
  <option name="--witness.yaml.validate">${benchmark_path_abs}/${rundefinition_name}/${taskdef_name}.witness.yml</option>

<rundefinition name="2ls">
  <tasks>
    <include>../plain/*.yml</include>
    <propertyfile expectedverdict="true">../properties/unreach-call.prp</propertyfile>
  </tasks>
</rundefinition>
