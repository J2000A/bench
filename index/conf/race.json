{
  "pre": {
    "cppflags": [
      "-D GOBLINT_NO_PTHREAD_ONCE",
      "-U __weak"
    ]
  },
  "ana": {
    "base": {
      "arrays": {
        "domain": "partitioned"
      }
    },
    "int": {
      "def_exc": true,
      "enums": false,
      "interval": true
    },
    "activated": [
      "base",
      "threadid",
      "threadflag",
      "mallocWrapper",
      "mutexEvents",
      "mutex",
      "access",
      "escape",
      "expRelation",
      "var_eq",
      "symb_locks",
      "mhp"
    ],
    "context": {
      "widen": false
    },
    "thread": {
	    "include-node": false
    },
    "opt": {
	    "hashcons": true
    },
    "malloc": {
      "wrappers": [
        "kmalloc",
        "__kmalloc",
        "usb_alloc_urb",
        "__builtin_alloca",
        "kzalloc",
        "ldv_malloc",
        "kzalloc_node",
        "ldv_zalloc",
        "kmalloc_array",
        "kcalloc"
      ]
    }
  },
  "exp": {
    "region-offsets": true
  },
  "solver": "td3",
  "solvers": {
    "td3": {
      "restart": {
        "wpoint": {
          "enabled": false,
          "once": true
        }
      },
      "abort": false,
      "abort-verify": false
    }
  },
  "incremental": {
    "reluctant": {
      "on": false,
      "compare": "equal"
    },
    "restart": {
      "sided": {
        "enabled": false,
        "only-global": false
      }
    }
  },
  "sem": {
    "unknown_function": {
      "spawn": false
    },
    "int": {
      "signed_overflow": "assume_none"
    }
  }
}
